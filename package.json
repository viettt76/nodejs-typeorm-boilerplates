{
    "name": "nodejs-typeorm-boilerplates",
    "version": "1.0.0",
    "description": "",
    "main": "server.ts",
    "scripts": {
		"dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/server.ts",
        "start": "node dist/server.js",
        "seed:run": "ts-node src/seeders/index.ts",
        "typeorm": "npx typeorm-ts-node-commonjs",
        "migration:generate": "npm run typeorm migration:generate -- -d src/data-source.ts",
        "migration:run": "npm run typeorm migration:run -- -d src/data-source.ts",
        "migration:revert": "npm run typeorm migration:revert",
        "drop-schema": "npm run typeorm schema:drop -- -d src/data-source.ts",
        "build": "tsc",
        "ts-check": "tsc --project tsconfig.json",
        "lint": "eslint 'src/**/*.ts'"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "bcrypt": "^6.0.0",
        "cookie": "^1.1.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^4.22.1",
        "express-async-errors": "^3.1.1",
        "joi": "^18.0.2",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.15.3",
        "redis": "^5.10.0",
        "reflect-metadata": "^0.2.2",
        "socket.io": "^4.8.1",
        "typeorm": "^0.3.28"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.10",
        "@types/express": "^5.0.6",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.10.1",
        "eslint": "^9.39.1",
        "nodemon": "^3.1.11",
        "prettier": "^3.7.4",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.3"
    }
}